<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
xmlns:fbutton="http://schemas.android.com/apk/res-auto"
tools:context="com.example.himanshu.myapplication.EnrichContentMain"
android:orientation="vertical"
android:weightSum="100">

<TextView
    android:id="@+id/presentTags"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="50dp"
    android:text="Original tags:"
    />
    <TextView
        android:id="@+id/TP"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="70dp"
        android:text="Add relevant tags:"
        />
<EditText
    android:id="@+id/newTagsText"
    android:layout_width="fill_parent"
    android:layout_height="40dp"
    android:layout_weight="10"
    android:hint="@string/newTags"/>
<com.example.himanshu.myapplication.FButton
    android:id="@+id/addNewTagsButton"
    android:layout_width="match_parent"
    android:layout_height="0dp"
    android:layout_weight="20"
    android:text="Add Tags"
    fbutton:buttonColor="@color/fbutton_color_peter_river"
    fbutton:shadowColor="@color/fbutton_color_belize_hole"
    fbutton:shadowEnabled="true"
    fbutton:shadowHeight="5dp"
    fbutton:cornerRadius="5dp"/>

<ImageView
    android:layout_width="match_parent"
    android:layout_height="0dp"
    android:layout_weight="45"
    android:id="@+id/imageViewReceived"

    android:contentDescription="@string/desc"/>

</LinearLayout>
